{
  "name": "next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache --cache-location ./node_modules/.cache/eslint/",
    "lint:fix": "eslint . --fix --cache --cache-location ./node_modules/.cache/eslint/",
    "lint:styles": "tsx src/scripts/check-unused-styles.ts",
    "format": "prettier --write . && yarn lint:fix",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/**/*.spec.{ts,tsx}",
    "test:integration": "vitest run src/**/*.integration.spec.{ts,tsx}",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "clean": "rm -rf .next node_modules/.cache coverage"
  },
  "dependencies": {
    "@grupo-ser/bff": "workspace:*",
    "@grupo-ser/shared": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "next": "^16.0.3",
    "react": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "reshaped": "^3.8.8",
    "sass": "^1.94.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^16.0.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "4.0.9",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "4.0.9",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  }
}
