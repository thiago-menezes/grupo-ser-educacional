{
  "name": "grupo-ser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache --cache-location ./node_modules/.cache/eslint/",
    "lint:fix": "eslint . --fix --cache --cache-location ./node_modules/.cache/eslint/",
    "format": "prettier --write . && yarn lint:fix",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/**/*.spec.{ts,tsx}",
    "test:integration": "vitest run src/**/*.integration.spec.{ts,tsx}",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@scalar/nextjs-api-reference": "^0.8.16",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "next": "^15.5.0",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "reshaped": "^3.8.5",
    "sass": "^1.90.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^5.14.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "happy-dom": "^20.0.8",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
